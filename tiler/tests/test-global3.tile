// This tests that global strings that have values assigned in one block can still be accessed in another block

string str;
int count;

init {
   tile(3,3);
   str = "test string";
   count = 0;
   sprint(str);
}

turn {
	sprint(str);
	count = count + 1;
	
}

end {

	if (count >= 1) {
		close();
		return 1;
	}
}