// This tests that the various if control flow works as expected

init {
	bool x;
	bool y;

	x = true;
	y = !x;

	tile(3,3);

	if(x){
		sprint("Twinkle, twinkle");
	}

	if(y){
		sprint("Lalala, lalala, Elmo's World");
	}

	if(!y){
		sprint("Little star");
	}

	if(3 > 2){
		sprint("How I wonder what you are");
	}

	if((1+1) == 3){
		sprint("Bow chika wow wow");
	}

	close();

}